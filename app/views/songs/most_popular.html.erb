<p id="notice"><%= notice %></p>

<h1 class = "text-center">Most Popular Songs</h1>

<table class="table table-striped table-inverse table-responsive">
  <thead class="thead-inverse">
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @top_most_popular.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td><%= link_to 'Show', song, class: "btn btn-success"  %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
